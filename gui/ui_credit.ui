   <string notr="true">QMenu {
	font: 11pt &quot;Lucida Casual&quot;;
 	background: #18191c;
  	border-radius: 4px;
  	border: 1px solid hsla(0, 0%, 100%, 0.3);
  	padding: 10px;
  	color: #b9bbbe;
}

QMenu::separator {
    height: 1px;
    background: hsla(0, 0%, 100%, 0.06);
    margin: 4px;
}
QMenu::item {
    color: #b9bbbe;
    /*background-color: #3d3d3d;*/
    border-radius: 2px;
    font-size: 14px;
    line-height: 18px;
    padding: 6px 8px;
    min-width: 120px;
    max-width: 240px;
}

QMenu::item:selected {
    background-color: #3d3d3d;
    color: white;
}

QMenu::item:disabled {
    color: #4f545c;
}
QMenu::icon {
    left: 7px;
	right: 7px;
}

QMenu::icon:checked {
   position: absolute;
   right: 7px;
   top: 1px;
   bottom: 1px;
   image-position: right;
   subcontrol-origin: padding;
   subcontrol-position: right center;
}</string>
