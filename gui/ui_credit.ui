	background-color: #383838;
}
QPushButton:pressed {	
	background-color: transparent;
	border: 2px solid #303030;
}
QPushButton:!enabled {
	color: #73797b;
	background-color: #202020;
}

QPushButton::menu-indicator {
	image: none;
}

